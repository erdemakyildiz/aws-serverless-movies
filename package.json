{
  "name": "aws-serverless-movies",
  "description": "AWS Server-less demo application / movie database.",
  "scripts": {
    "test:coverage": "nyc --check-coverage --lines 90 npm run test:unit",
    "report": "nyc report --reporter=html",
    "test:unit": "blue-tape test/**/**.spec.js | tap-spec",
    "style": "standard \\\"src/**/*.js\\\" | snazzy",
    "style:fix": "standard \\\"src/**/*.js\\\" --fix",
    "build": "npm prune --production && mkdir -p build && pushd src && zip -r ../build/lambda.zip . ../node_modules && popd && npm install"
  },
  "dependencies": {

  },
  "devDependencies": {
    "nyc": "^11.0.1",
    "blue-tape": "^1.0.0",
    "tap-spec": "^4.1.1",
    "standard": "^10.0.3",
    "babel-eslint": "^7.2.3",
    "snazzy": "^7.0.0"
  },
  "peerDependencies": {},
  "standard": {
    "parser": "babel-eslint"
  },
  "author": "Ivo Houbrechts",
  "version": "0.1.0",
  "license": "APACHE"
}

